        <li>r_name(p=2)->[BNF_Exoduster_FirstM] [BNF_Exoduster_Last]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>BNF_Namer_Exoduster_Female</defName>
    <include>
      <li>BNF_PawnNamerExoduster</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>r_name(p=2)->[BNF_Exoduster_FirstF] [BNF_Exoduster_Last]</li>
      </rulesStrings>
    </rulePack>
